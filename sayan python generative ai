        "#my_list[SI:EI:SS]\n",
        "#SI->Starting index,it is included.\n",
        "#EI->Ending index,it is excluded.\n",
        "#SS->Step size.\n",
        "\n",
        "# Example of slicing\n",
        "my_list = [98, 87, 76, 65, 54, 43]\n",
        "print(my_list[1:4:1])"
